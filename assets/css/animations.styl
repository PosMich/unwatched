@keyframes fade-in-delayed
    0% { 
        opacity 0
    }
    50% { 
        opacity 0
    }
    100% { 
        opacity 1
    }

@keyframes fade-item-in
    0% { 
        opacity 0
        margin-left -5%
    }
    60% {
        opacity 0.6
        margin-left 0%
    }
    100% { 
        opacity 1
    }

@keyframes fade-out
    from { 
        opacity 1
    }
    to { 
        opacity 0
    }