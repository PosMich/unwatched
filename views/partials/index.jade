#create-room-container.well(center-vertical)
    form.form-horizontal
        fieldset
            legend Create Room
            .form-group
                .col.span_2_of_12
                label.col.span_2_of_12.control-label(for='inputEmail') Email
                .col.span_6_of_12
                    input#inputEmail.form-control(type='text', placeholder='Email')
                .col.span_2_of_12
            .form-group
                .col.span_2_of_12
                label.col.span_2_of_12.control-label(for='inputPassword') Password
                .col.span_6_of_12
                    input#inputPassword.form-control(type='password', placeholder='Password')
                .col.span_2_of_12
            .form-group
                .col.span_1_of_12
                .col.span_10_of_12
                    button.btn.btn-default Cancel
                    button.btn.btn-primary(type='submit') Submit
                .col.span_1_of_12
